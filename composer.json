{
    "name": "manuskript/manuskript",
    "description": "Build your admin panel into your Laravel application",
    "keywords": [
        "laravel",
        "admin-panel"
    ],
    "license": "MIT",
    "homepage": "https://manuskript.dev",
    "support": {
        "issues": "https://github.com/orgs/manuskript/discussions",
        "source": "https://github.com/manuskript/manuskript"
    },
    "authors": [
        {
            "name": "Tobias Thaden",
            "email": "tobias@manuskript.dev"
        }
    ],
    "autoload": {
        "psr-4": {
            "Manuskript\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Manuskript\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.75"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Manuskript\\ServiceProvider"
            ]
        }
    }
}
